{
  "manifest_version": 3,
  "name": "Experimental PG Panda",
  "version": "1.0",
  "description": "A Google Chrome extension for parental control.",
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "webRequest",
    "storage",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
    
  }
}